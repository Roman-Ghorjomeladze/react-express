{
  "name": "Deel",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "sequelize": "^6.35.1",
    "sqlite3": "^5.1.6"
  },
  "scripts": {
    "prebuild": "npm run format",
    "build": "tsc",
    "start": "npm run build && npm run build --prefix=client && node ./dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "install:both": "npm install --prefix=client && npm install",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "seed": "npm run build && node dist/scripts/seedDb.js"
  },
  "engines": {
    "node": ">=10.16.3"
  },
  "engineStrict": true,
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.1",
    "@types/sequelize": "github:types/sequelize",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
